        "@material-ui/core": "^4.7.2",
        "@material-ui/icons": "^4.5.1",
        "@opetushallitus/virkailija-ui-components": "^0.4.7",
        "@tanstack/react-table": "^8.5.13",
        "axios": "^0.28.1",
        "axios-hooks": "^4.0.0",
        "classnames": "^2.5.1",
        "date-fns": "^2.30.0",
        "email-validator": "^2.0.4",
        "lodash": "^4.17.21",
        "normalize.css": "^8.0.1",
        "oph-virkailija-style-guide": "github:Opetushallitus/virkailija-styles#master",
        "react": "^16.14.0",
        "react-datepicker": "^2.9.6",
        "react-dom": "^16.14.0",
        "react-router": "^5.3.3",
        "react-router-dom": "^5.3.3",
        "react-select": "^3.0.8",
        "react-toastify": "^9.1.3",
        "styled-components": "^5.3.6",
        "universal-cookie": "^4.0.2",
        "use-debounce": "^3.1.0"
    },
    "devDependencies": {
        "@types/lodash": "^4.17.1",
        "@types/node": "^20.12.11",
        "@types/react": "^16.14.60",
        "@types/react-beforeunload": "^2.1.5",
        "@types/react-datepicker": "2.9.5",
        "@types/react-dom": "^16.9.24",
        "@types/react-router": "^5.1.20",
        "@types/react-router-dom": "^5.3.3",
        "@types/react-select": "3.0.8",
        "@types/styled-components": "5.1.26",
        "@types/styled-system": "^5.1.22",
        "@typescript-eslint/eslint-plugin": "^7.8.0",
        "@typescript-eslint/parser": "^7.8.0",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^6.11.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.34.1",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^9.0.2",
        "html-webpack-plugin": "^5.6.0",
        "mini-css-extract-plugin": "^2.9.0",
        "prettier": "^3.2.5",
        "source-map-loader": "^5.0.0",
        "style-loader": "^4.0.0",
        "ts-loader": "^9.4.2",
        "tsx": "^4.10.2",
        "typescript": "^4.9.5",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4",
        "webpack-manifest-plugin": "^5.0.0"
    },
    "scripts": {
        "start": "NODE_ENV=development npx webpack serve",
        "build": "GENERATE_SOURCEMAP=false NODE_ENV=production npx webpack --mode production",
        "test": "node --import tsx --test ./src/**/*.test.ts",
        "lint": "tsc --noEmit && eslint --ext .ts,.tsx src",
        "lint:fix": "eslint --ext .ts,.tsx src --fix",
        "prettier": "prettier --check 'src/**/*{js,jsx,ts,tsx}'",
        "prettier:fix": "prettier --write 'src/**/*{js,jsx,ts,tsx}'"
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "plugins": [
            "@typescript-eslint",
            "react"
        ],
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/eslint-recommended",
            "plugin:@typescript-eslint/recommended",
            "plugin:react/recommended"
        ],
        "settings": {
            "react": {
                "version": "detect"
            }
        },
        "rules": {
            "@typescript-eslint/no-unused-vars": [
                "error",
                {
                    "ignoreRestSiblings": true
                }
            ]
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
