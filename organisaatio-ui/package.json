{
    "homepage": "/organisaatio-service",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@hookform/resolvers": "^2.5.2",
        "@iconify/icons-fa-solid": "1.1.1",
        "@iconify/react": "3.1.0",
        "@joi/date": "^2.1.0",
        "@opetushallitus/virkailija-ui-components": "^0.4.6",
        "axios": "^1.6.7",
        "axios-hooks": "^5.0.2",
        "joi": "^17.4.0",
        "joi-phone-number": "^5.1.1",
        "jotai": "^1.5.3",
        "moment": "^2.29.4",
        "oph-urls-js": "github:Opetushallitus/oph-urls-js#master",
        "query-string": "^7.0.1",
        "react": "^16.14.0",
        "react-accessible-accordion": "3.3.3",
        "react-datepicker": "3.2.2",
        "react-dom": "^16.14.0",
        "react-hook-form": "^7.8.4",
        "react-notifications-component": "^2.4.1",
        "react-router-dom": "5.2.0",
        "react-select": "^5.8.0",
        "react-table": "7.2.0",
        "react-window": "^1.8.10",
        "reactjs-popup": "^2.0.5",
        "styled-components": "5.1.1",
        "universal-cookie": "4.0.4"
    },
    "devDependencies": {
        "@cypress/code-coverage": "^3.12.1",
        "@cypress/instrument-cra": "^1.4.0",
        "@types/jest": "^27",
        "@types/joi-phone-number": "^5.0.6",
        "@types/node": "^16.18.54",
        "@types/react": "^16.14.47",
        "@types/react-datepicker": "2.11.1",
        "@types/react-dom": "^16.9.20",
        "@types/react-notifications-component": "^3.1.2",
        "@types/react-router-dom": "5.1.5",
        "@types/react-table": "^7.0.19",
        "@types/react-window": "^1.8.8",
        "@types/styled-components": "4.4.0",
        "@typescript-eslint/eslint-plugin": "^6.4.1",
        "@typescript-eslint/parser": "^6.4.1",
        "concurrently": "^6.0.2",
        "copy-webpack-plugin": "^12.0.2",
        "css-loader": "^6.11.0",
        "cypress": "^10.11.0",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-jsx-a11y": "^6.8.0",
        "eslint-plugin-react": "^7.34.1",
        "file-loader": "^6.2.0",
        "fork-ts-checker-webpack-plugin": "^9.0.2",
        "html-webpack-plugin": "^5.6.0",
        "mini-css-extract-plugin": "^2.9.0",
        "finnish-business-ids": "^3.1.1",
        "prettier": "2.2.1",
        "pretty-quick": "^3.1.2",
        "source-map-loader": "^5.0.0",
        "style-loader": "^4.0.0",
        "ts-loader": "^9.4.2",
        "typescript": "^4.9.5",
        "wait-on": "^7.2.0",
        "webpack": "^5.91.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^5.0.4",
        "webpack-manifest-plugin": "^5.0.0"
    },
    "scripts": {
        "start": "NODE_ENV=development npx webpack serve",
        "build": "GENERATE_SOURCEMAP=false NODE_ENV=production npx webpack --mode production",
        "lint": "tsc --noEmit && eslint --ext .ts,.tsx src",
        "lint:fix": "eslint --ext .ts,.tsx src --fix",
        "prettier": "prettier --check 'src/**/*{js,jsx,ts,tsx}'",
        "prettier:fix": "prettier --write 'src/**/*{js,jsx,ts,tsx}'",
        "audit": "npm audit --only=prod --audit-level=critical",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run",
        "cypress:notused": "wait-on http://devaaja:devaaja@localhost:8080/organisaatio-service && CYPRESS_BASE_URL=http://devaaja:devaaja@localhost:8080/ npm run cypress:run",
        "cypress:ci": "wait-on http://localhost:3003/organisaatio-service && npm run cypress:run"
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "plugins": [
            "@typescript-eslint",
            "cypress",
            "jest",
            "react"
        ],
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/eslint-recommended",
            "plugin:@typescript-eslint/recommended",
            "plugin:react/recommended",
            "plugin:cypress/recommended",
            "plugin:jest/recommended"
        ],
        "settings": {
            "react": {
                "version": "detect"
            }
        },
        "rules": {
            "@typescript-eslint/no-unused-vars": [
                "error",
                {
                    "ignoreRestSiblings": true
                }
            ]
        }
    },
    "proxy": "http://localhost:8080",
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "scarfSettings": {
        "enabled": false
    },
    "jest": {
        "roots": [
            "<rootDir>/src"
        ],
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,ts,tsx}",
            "!src/**/*.d.ts"
        ],
        "setupFiles": [
            "react-app-polyfill/jsdom"
        ],
        "setupFilesAfterEnv": [],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
            "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
        ],
        "testEnvironment": "jsdom",
        "transform": {
            "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ],
        "modulePaths": [],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
        },
        "moduleFileExtensions": [
            "web.js",
            "js",
            "web.ts",
            "ts",
            "web.tsx",
            "tsx",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ],
        "resetMocks": true,
        "coverageReporters": [
            "lcov"
        ]
    },
    "nyc": {
        "report-dir": "cypress-coverage"
    },
    "engines": {
        "npm": "^10",
        "node": "^20"
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    }
}
