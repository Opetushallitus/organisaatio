            <xs:element minOccurs="0" name="searchStr" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>


    <xs:complexType name="organisaatioSearchCriteriaDTO">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Organisaatioiden hakuehdot.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="searchCriteriaDTO">
                <xs:sequence>
                    <xs:element name="lakkautetut" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation xml:lang="fi">Jos totta, niin palautetaan myös lakkautetut
                                organisaatiot.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="oppilaitosTyyppi" type="xs:string">
                        <xs:annotation>
                            <xs:documentation xml:lang="fi">Jos annettuna, niin rajataan oppilaitostyypin (koodistosta)
                                mukaan.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="organisaatioTyyppi" type="xs:string">
                        <xs:annotation>
                            <xs:documentation xml:lang="fi">Jos annettu, niin rajataan hakua OrganisaatioTyypin (enum)
                                mukaan.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="suunnitellut" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation xml:lang="fi">Jos totta, niin palautetaan ei vielä voimassaolevia
                                organisaatioita (alkamisPvm tulevaisuudessa).
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="firstResult" type="xs:int">
                        <xs:annotation>
                            <xs:documentation xml:lang="fi">Ensimmäisen tuloksen offset (sivutukseen).
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="organisaatioDomainNimi" type="xs:string">
                        <xs:annotation>
                            <xs:documentation xml:lang="fi">Haetaan organisaation domain nimestä.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="kunta" type="xs:string">
                        <xs:annotation>
                            <xs:documentation xml:lang="fi">Haetaan valitun kunnan koodisto uri:lla.</xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" maxOccurs="unbounded" name="oidResctrictionList" type="xs:string">
                        <xs:annotation>
                            <xs:documentation xml:lang="fi">Jos annettu, niin hakutulosten on oltava annetussa
                                listassa.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>

    <xs:complexType name="organisaatioPerustietoType">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Organisaation "kevyet" perustiedot.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="1" maxOccurs="1" name="oid" type="xs:string"/>
            <xs:element minOccurs="1" maxOccurs="1" name="version" type="xs:long"/>
            <xs:element minOccurs="0" name="alkuPvm" type="xs:dateTime"/>
            <xs:element minOccurs="0" name="lakkautusPvm" type="xs:dateTime"/>
            <xs:element minOccurs="0" name="nimiEn" type="xs:string"/>
            <xs:element minOccurs="0" name="nimiFi" type="xs:string"/>
            <xs:element minOccurs="0" name="nimiSv" type="xs:string"/>
            <xs:element minOccurs="0" name="parentOid" type="xs:string"/>
            <xs:element minOccurs="0" name="parentOidPath" type="xs:string"/>
            <xs:element minOccurs="0" name="ytunnus" type="xs:string"/>
            <xs:element minOccurs="0" name="virastoTunnus" type="xs:string"/>

            <xs:element minOccurs="1" maxOccurs="1" name="aliOrganisaatioMaara" type="xs:int"/>
            <xs:element minOccurs="0" name="oppilaitosKoodi">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:pattern value="[0-9]{5}"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element minOccurs="0" name="oppilaitostyyppi" type="xs:string"/>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="tyypit" nillable="true" type="organisaatioTyyppi"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="organisaatioKuvaTyyppi">
        <xs:annotation>
            <xs:documentation xml:lang="fi">Organisaatio kuva.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="1" name="mimeType" type="xs:string"/>
